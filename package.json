{
    "name": "format-json",
    "displayName": "format-json",
    "publisher": "freedom-compile",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.65.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:format-json"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [{
            "command": "format-json",
            "title": "Format the selected JSON data"
        }],
        "keybindings": [{
            "command": "format-json",
            "key": "ctrl+shift+j",
            "mac": "cmd+shift+j",
            "when": "editorHasSelection"
        }],
        "configuration": {
            "title": "format-json.configuration",
            "type": "object",
            "properties": {
                "format-json.configuration.space_count": {
                    "type": "integer",
                    "default": 2,
                    "description": "缩进空格数"
                }
            }
        }
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.65.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "eslint": "^8.9.0",
        "glob": "^7.2.0",
        "mocha": "^9.2.1",
        "typescript": "^4.5.5",
        "@vscode/test-electron": "^2.1.2"
    }

}